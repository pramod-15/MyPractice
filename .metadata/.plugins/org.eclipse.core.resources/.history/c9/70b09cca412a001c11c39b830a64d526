package com.in28minutes.oopsAgain;

public class RectangleRunner {

	public static void main(String[] args) {

		Rectangle rect = new Rectangle(12, 40);
		System.out.println(rect);
		rect.setLength(50);
		System.out.println(rect);
		rect.setWidth(15);
		System.out.println(rect);
		System.out.println(rect);


	}
}