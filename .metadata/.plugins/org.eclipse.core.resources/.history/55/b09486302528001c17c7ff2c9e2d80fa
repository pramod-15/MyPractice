package com.in28minutes.oopsAgain;

public class Fan {
	
	
	private String make;
	private double radius;
	private String color;
	
	private boolean isOn;
	private short speed;
	
	public Fan(String make, double rad, String color) {
		
		this.make = make;
		this.radius = rad;
		this.color = color;
	}
	
}
